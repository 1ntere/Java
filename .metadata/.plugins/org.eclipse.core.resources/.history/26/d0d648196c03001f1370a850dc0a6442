package com.kh.ThreadEx;

//Thread 클래스 가져오는 작업 진행
/*면접단골질문*/
public class ThreadEx2 extends Thread {
//필드
	int 초;
//메서드
	//필수 생성자 : alt + shift + s -> o
	public ThreadEx2(int 초) {
		this.초 = 초;
	}
	//void run
	public void run() {
		System.out.println(this.초 + " 스레드 시작 중~~");
		
		Thread.sleep(1000);
	}
}
